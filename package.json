{
  "name": "brandshop",
  "version": "1.0.0",
  "main": "dist/index",
  "scripts": {
    "start:dev": "node -r dotenv/config --inspect=0.0.0.0:9227 ./dist/",
    "start": "rimraf dist && npm run lint && copyfiles -e src/email-templates/*.ts -f src/email-templates/*.* dist/email-templates/ && tsc-watch --outDir ./dist --onSuccess \"npm run start:dev\" --onFailure \"'echo Compilation Failed'\"",
    "build": "rimraf dist && tsc",
    "tsc": "rimraf dist && npm run lint && tsc --watch",
    "lint": "eslint --fix --ext .ts,.js src"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "16.0.1",
    "email-templates": "^9.0.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "pug": "^3.0.2",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.6",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/email-templates": "^8.0.4",
    "@types/express": "4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/pug": "^2.0.6",
    "@types/helmet": "^4.0.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.27.1",
    "@typescript-eslint/parser": "^5.28.0",
    "@types/bcrypt": "^5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "tsc-watch": "^5.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.7.3"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
